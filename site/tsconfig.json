{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": []
  },
  "exclude": [
    "node_modules",
    "**/*.d.ts",
    "../node_modules",
    "../../node_modules",
    "../../../.yarn"
  ],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
